{
  "vim.easymotion": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "underline",
  "vim.cursorStylePerMode.visualblock": "line-thin",
  "vim.cursorStylePerMode.visualline": "block-outline",
  "vim.cursorStylePerMode.replace": "underline",
  "vim.easymotionDimBackground": true,
  "vim.easymotionDimColor": "#41505e",
  "vim.easymotionMarkerFontWeight": "900",
  "vim.easymotionMarkerForegroundColorOneChar": "#eb598d",
  "vim.easymotionMarkerBackgroundColor": "#101010",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#b0d2fb",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#cbe9fd",
  "vim.normalModeKeyBindingsNonRecursive": [
    // File Utils
    {
      "before": ["<leader>", "n", "f"],
      "commands": ["fileutils.newFile"]
    },
    {
      "before": ["<leader>", "n", "d"],
      "commands": ["fileutils.newFolder"]
    },
    {
      "before": ["<leader>", "f", "n"],
      "commands": ["fileutils.renameFile"]
    },
    // Extension: Bookmarks
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["m", "a"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["m", "c", "c"],
      "commands": ["bookmarks.clear"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    // 控制activityBar可见
    {
      "before": ["<leader>", "a"],
      "commands": [
        // "workbench.action.toggleStatusbarVisibility",
        "workbench.action.toggleActivityBarVisibility"
        // "editor.action.toggleMinimap",
        // "workbench.action.toggleFullScreen"
      ]
    },

    {
      "before": ["/"],
      "after": ["<leader>", "<leader>", "/"]
    },
    // 快速上下行
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    // 检索
    {
      "before": ["F"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "<cr>"],
      "commands": [":nohlsearch"]
    },
    {
      "before": ["-"],
      "after": ["N"]
    },
    {
      "before": ["="],
      "after": ["n"]
    },
    // 格式化文件
    {
      "before": ["\\", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    // 当前标签切换
    {
      "before": ["g", "l"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["g", "h"],
      "commands": ["workbench.action.previousEditor"]
    },
    // 侧边栏可见行
    {
      "before": ["t", "t"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["N", "N"],
      "after": ["<esc>"]
    },
    // 移动当前行
    {
      "before": ["m", "j"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["m", "k"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    // 进入full secreen
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.toggleFullScreen"]
    },
    // easy motion to placeholder
    {
      "before": ["`"],
      "after": ["<leader>", "<leader>", "s", "\\", "c", "l"]
    },
    {
      "before": ["`", "`"],
      "after": ["<leader>", "<leader>", "s", "\\"]
    },
    // 显示悬停
    {
      "before": ["\\"],
      "commands": ["editor.action.showHover"]
    },
    // 重命名
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    // 重做
    {
      "before": ["U"],
      "after": ["<C-r>"]
    },
    // 速览定义
    {
      "before": ["s", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    // // 折叠代码块
    // {
    //   "before": ["<tab>"],
    //   "commands": ["editor.toggleFold"]
    // },
    // 多窗口
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<C-w>", "t"],
      "commands": ["workbench.action.toggleEditorWidths"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    // 保存、关闭文件
    {
      "before": ["W"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["Q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    // 打开、关闭状态栏
    {
      "before": [":"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": [";"],
      "commands": ["workbench.action.showCommands"]
    },
    // easy motion
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "s"]
    },
    // 选择下一个
    {
      "before": ["<leader>", "d"],
      "commands": [
        "editor.action.addSelectionToNextFindMatch",
        "editor.action.addSelectionToNextFindMatch"
      ]
    },
    {
      "before": ["N", "n"],
      "after": ["<esc>"]
    },
    {
      "before": ["N", "N"],
      "after": ["<esc>"]
    }
  ],
  "vim.leader": "<space>",
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["N", "n"],
      "after": ["<esc>"]
    },
    {
      "before": ["N", "N"],
      "after": ["<esc>"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<left>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<down>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<up>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<right>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [";"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["N", "n"],
      "after": ["<esc>"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    // 移动当前行
    {
      "before": ["m", "j"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["m", "k"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["compile-hero.openInBrowser"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["N", "N"],
      "after": ["<esc>"]
    },
    {
      "before": ["/"],
      "commands": ["editor.action.commentLine"]
    },
    // 句首，句尾跳转
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    // 选中相似的下一个
    {
      "before": ["<leader>", "d"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    }
  ],
